echo Plug your iOS device into your Mac. Waiting 10 seconds.
sleep 10
read -p "Enter your UDID: " uniquedeviceid
ideviceenterrecovery $uniquedeviceid
echo  entering recovery. waiting 10 seconds
sleep 10
echo entered recovery. dumping info
irecovery -q | grep NONC
irecovery -n
sleep 25
ideviceinfo | grep UniqueChipID
ideviceinfo | grep ProductType
echo got info.
